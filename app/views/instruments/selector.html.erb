<div class="row">
	<div class="col-md-6">
		<h4>Instruments</h4>
		<select class="inst-selector" multiple> <!-- all instruments -->
		<% Instrument.all.each do |inst| %> 
			<option ondblclick="clickedInstrument()"><%= render inst %></option>
		<% end %>
		</select>
	</div>
	<div class="col-md-6"> <!-- user instruments -->
		<h4>My Instruments</h4>
		<ul id="user-instruments">
		<% @instruments.each do |user_inst| %>
			<%= render user_inst %>
		<% end %>
		</ul> 
	</div>
</div>